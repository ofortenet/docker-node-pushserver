{
    "webPort": 8080,

    "gcm": {
        "apiKey": "process.env.apiKey"
    },

    "apn": {
        "connection": {
            "gateway": "gateway.push.apple.com",
            "cert": "/usr/pushserver/cert.pem",
            "key": "/usr/pushserver/key.pem"
        },
        "feedback": {
            "address": "feedback.push.apple.com",
            "cert": "/usr/pushserver/cert.pem",
            "key": "/usr/pushserver/key.pem",
            "interval": 43200,
            "batchFeedback": true
        }
    }
}
